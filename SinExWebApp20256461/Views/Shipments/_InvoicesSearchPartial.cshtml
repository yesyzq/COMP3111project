@model SinExWebApp20256461.ViewModels.InvoicesSearchViewModel

@using (Html.BeginForm("GenerateInvoiceReport", "Shipments", FormMethod.Get))
{
    @Html.LabelFor(a => a.ShippingAccountNumber, "Shipping Account")
    @Html.DropDownListFor(a => a.ShippingAccountNumber, Model.ShippingAccounts, "please Select")
    @Html.TextBoxFor(a => a.ShippedStartDate, htmlAttributes: new { @value = ViewBag.CurrentShippingStartDate})
    @Html.TextBoxFor(a => a.ShippedEndDate, htmlAttributes: new { @value = ViewBag.CurrentShippingEndDate })
    <input type="submit" value="Search" />
}

